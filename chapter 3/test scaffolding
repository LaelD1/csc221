  >>> thing[2] = 17
  >>> thing[2]
  17
"""

"""
  >>> tricky[4]
  'this'
  >>> tricky[8]
  42
  >>> type(tricky[7])
  <class 'tuple'>
  >>> type(tricky[0])
  <class 'list'>
  >>> len(tricky)
  12
"""

