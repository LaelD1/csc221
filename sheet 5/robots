from gasp import *          # So that you can draw things
from random import randint
def place_robot():
    robot_x = randint(0, 63)
    robot_y = randint(0, 47)
    Box(10, 10, 10, filled=False, color=color.Black, thickness=1)

def place_player():
    print("Here I am!")
    player_x = randint(0, 63)
    player_y = randint(0, 47)
    Circle((10 * player_x + 5, 10 * player_y + 5), 5, filled=True)


def move_player():
    print("I'm moving...")
    update_when('key_pressed')
    
    




begin_graphics()            # Create a graphics window
finished = False

place_player()

while not finished:
    move_player()


end_graphics()              