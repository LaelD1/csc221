from gasp import *          # So that you can draw things

begin_graphics()
ball_x = 5
ball_y = 5
c = Circle((ball_x, ball_y), 5, filled=True)

while ball_x<635 and ball_y<475:
    ball_x = ball_x+4
    ball_y = ball_y+3

    move_to(c, (ball_x, ball_y))
    sleep(0.02)

remove_from_screen(c)

end_graphics()              